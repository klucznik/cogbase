{
	"name": "klucznik/cogbase",
	"license": "MIT",
	"description": "Base package providing type casting, utils and exceptions for use in other cog packages",
	"require": {
		"php": ">=5.4",
		"nesbot/carbon": "^1.22.0",
		"danielstjules/stringy": "^3.1.0",
		"symfony/filesystem": "^3.0",
		"symfony/http-foundation": "^3.0",
		"league/url": "^3.3.5"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7.27",
		"symfony/var-dumper": "^3.0",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Cog\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cog\\Test\\": "tests"
		}
	}
}
