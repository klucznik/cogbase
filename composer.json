{
	"name": "klucznik/cogbase",
	"license": "MIT",
	"description": "Base package providing type casting, various utils and exceptions for use in other cog packages",
	"require": {
		"php": ">=7.2",
		"nesbot/carbon": "^2.0",
		"symfony/filesystem": "^4.3|^5.0",
		"symfony/http-foundation": "^4.4|^5.0",
		"symfony/mime": "^4.4|^5.0",
		"league/uri": "^6.0",
		"league/uri-components": "^2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.4",
		"statamic/stringy": "^3.1",
		"symfony/var-dumper": "^4.0|5.0",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Cog\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cog\\Test\\": "tests"
		}
	}
}
