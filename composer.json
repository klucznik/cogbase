{
  "name": "klucznik/cogbase",
  "license": "MIT",
  "description": "Base package providing type casting, utils and exceptions for use in other cog packages",
  "require": {
    "php": ">=5.4",
    "nesbot/carbon": "^1.21.0",
    "danielstjules/stringy": "^2.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.1",
    "symfony/var-dumper": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Cog\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cog\\Test\\": "tests"
    }
  }
}
