{
	"name": "klucznik/cogbase",
	"license": "MIT",
	"description": "Base package providing type casting, various utils and exceptions for use in other cog packages",
	"require": {
		"php": ">=7.2",
		"nesbot/carbon": "^2.0",
		"statamic/stringy": "^3.1",
		"symfony/filesystem": "^4.0",
		"symfony/http-foundation": "^4.0",
		"league/uri": "^5.3"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.0",
		"symfony/var-dumper": "^4.0",
		"phpstan/phpstan": "^0.11.2",
		"roave/security-advisories": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Cog\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Cog\\Test\\": "tests"
		}
	}
}
